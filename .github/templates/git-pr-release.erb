[Release] <%= Time.now.localtime("+09:00").strftime("%Y-%m-%d %H:%M:%S") %>

<% labels = {feature: 'feature', bugfix: 'bugfix', migration: 'migration'}-%>

## 機能開発
<% pull_requests.map{ |pr| pr.labels.filter{ |l| l.name == labels[:feature]} } do |pr| -%>
  <%= "- [#{pr.title}](#{pr.html_url}) by #{pr.mention}" %>
<% end -%>

## 不具合修正
<% pull_requests.map{ |pr| pr.labels.filter{ |l| l.name == labels[:bugfix]} } do |pr| -%>
  <%= "- [#{pr.title}](#{pr.html_url}) by #{pr.mention}" %>
<% end -%>

## マイグレーション
<% pull_requests.map{ |pr| pr.labels.filter{ |l| l.name == labels[:migration]} } do |pr| -%>
  <%= "- [#{pr.title}](#{pr.html_url}) by #{pr.mention}" %>
<% end -%>

## その他
<% pull_requests.map{ |pr| pr.labels.reject{ |l| l.name.include?(labels.values)} } do |pr| -%>
  <%= "- [#{pr.title}](#{pr.html_url}) by #{pr.mention}" %>
<% end -%>
